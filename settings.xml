<settings>
    <mirrors>
        <mirror>
            <id>artifactory</id>
            <mirrorOf>*</mirrorOf>
            <url>https://your-artifactory-url/artifactory/maven</url>
            <repositoryManager>true</repositoryManager>
        </mirror>
    </mirrors>
    
    <servers>
        <server>
            <id>artifactory</id>
            <username>your-username</username>
            <password>your-password</password>
        </server>
    </servers>
</settings>

